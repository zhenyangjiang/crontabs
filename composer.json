{
    "name": "ulan/crontabs",
    "description": "ULan Crontabs apps",
    "keywords": [
        "ULan",
        "Crontabs"
    ],
    "type" : "project",
    "require": {
        "phpmailer/phpmailer": "~5.2",
        "pda/pheanstalk": "^3.1",
        "predis/predis": "^1.0",
        "nesbot/carbon": "^1.21",
        "ender/yunpian-sms": "^1.0",
        "landers/substrate": "dev-master",
        "landers/framework": "dev-master",
        "landers/laravel": "dev-master",
        "landers/ulan": "dev-master",
        "guzzlehttp/guzzle": "dev-master",
        "ulan/virtsdk" : "dev-master",
        "ulan/repository" : "dev-master",
        "ulan/hprose": "dev-master"
    },
    "autoload":{
        "classmap": [
            "package/Repositories"
        ],
        "psr-4": {
            "Tasks\\" : "package/Tasks/",
            "Services\\" : "package/Services/"
        },
        "files": [
            "vendor/landers/framework/src/Adapters/Helpers/LcliHelpers.php",
            "package/helpers.php"
        ]
    },
   "repositories": [

    ],
    "repositories": {
        "landers-substrate" : {
            "type": "vcs",
            "url": "https://git.ulan.io/landers/substrate.git"
        },
        "landers-framework" : {
            "type": "vcs",
            "url": "https://git.ulan.io/landers/framework.git"
        },
        "landers-laravel" : {
            "type": "vcs",
            "url": "https://git.ulan.io/landers/laravel.git"
        },
        "landers-ulan" : {
            "type": "vcs",
            "url": "https://git.ulan.io/landers/ulan.git"
        },
        "ulan-virtsdk" : {
            "type": "vcs",
            "url": "https://git.ulan.io/packages/virtsdk.git"
        },
        "ulan-repository" : {
            "type": "vcs",
            "url": "https://git.ulan.io/packages/repository.git"
        },
        "ulan-hprose": {
          "type": "vcs",
          "url": "https://git.ulan.io/packages/hprose.git"
        },
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }

}
